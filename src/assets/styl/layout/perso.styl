.perso
	width 240px
	height 335px
	position absolute
	perspective 2600px
	-webkit-perspective 2600px
	perspective-origin 20% 50%
	top 0
	left 0
	transition all 500ms ease

/*
* Fond
*/
#fond
	position absolute
	width 100%
	transform-style preserve-3d
	animation initRotate 3500ms cubic-bezier(0.645, 0.045, 0.355, 1)
	-webkit-perspective 2600px
	perspective 2600px

#fond #foeil,
#fond #visage,
#fond #fbonnet,
#fond #fcache,
#fond #fbouche,
#fond #fbic
	position absolute
	top 0
	left 0

#fond #fvisage .st0
	fill #F9C9B2

#fond #fvisage .st1
	fill #E8BBAC

#fond #foeil .st0
	fill #FFFFFF

#fond #foeil .st1
	fill #424242

#fond #fbonnet .st1
	fill #424242

#fond #fbonnet .st0
	fill #D6DDE5

#fond #fbonnet .st1
	fill #B8C3CC

#fond #fbonnet .st2
	fill #C6D0DA

#fond #fbouche
	transform-style preserve-3d
	animation translateZa 3500ms cubic-bezier(0.645, 0.045, 0.355, 1)

#fond #foeil
	transform-style preserve-3d
	animation translateZb 3500ms cubic-bezier(0.645, 0.045, 0.355, 1)

#fond #fcache
	transform-style preserve-3d
	animation translateZc 3500ms cubic-bezier(0.645, 0.045, 0.355, 1)

#fond #fbonnet
	transform-style preserve-3d
	animation translateZd 3500ms cubic-bezier(0.645, 0.045, 0.355, 1)

#fond #fbic
	transform-style preserve-3d
	animation translateZe 3500ms cubic-bezier(0.645, 0.045, 0.355, 1)

@keyframes initTranslate
	to 	{ transform: translateX(0px) translateY(0px) translateZ(0px); }

@keyframes initRotate
	from { transform: rotateX(50deg) rotateY(0deg) rotateZ(-15deg); }
	30% { transform: rotateX(50deg) rotateY(0deg) rotateZ(-15deg); }
	to 	{ transform: rotateX(0deg) rotateY(0deg) rotateZ(0deg); }

@keyframes translateZa
	from { transform : translateX(0px) translateY(0px) translateZ(60px); opacity: 0; }
	30% { transform : translateZ(20px) scale(1); opacity: 1; }
	to 	{ transform: translateX(0px) translateY(0px) translateZ(0px); }

@keyframes translateZb
	from { transform: translateX(0px) translateY(0px) translateZ(90px); opacity: 0; }
	30% { transform : translateZ(40px) scale(1); opacity: 1; }
	to 	{ transform: translateX(0px) translateY(0px) translateZ(0px); }

@keyframes translateZc
	from { transform: translateX(0px) translateY(0px) translateZ(120px); opacity: 0; }
	30% { transform : translateZ(80px) scale(1); opacity: 1; }
	to 	{ transform: translateX(0px) translateY(0px) translateZ(0px); }

@keyframes translateZd
	from { transform: translateX(0px) translateY(0px) translateZ(150px); opacity: 0; }
	30% { transform : translateZ(100px) scale(1); opacity: 1; }
	to 	{ transform: translateX(0px) translateY(0px) translateZ(0px); }

@keyframes translateZe
	from { transform: translateX(0px) translateY(0px) translateZ(180px); opacity: 0; }
	30% { transform : translateZ(120px) scale(1);opacity: 1; }
	to 	{ transform: translateX(0px) translateY(0px) translateZ(0px); }

/*
* Contour
*/
#contour,
.contour-blanc,
#contour-blanc
	position absolute
	width 100%
	transform-style preserve-3d
	-webkit-perspective 2600px
	perspective 2600px

#contour .st0
	fill none
	stroke #424242
	stroke-width 3
	stroke-miterlimit 10

#contour .st1
	fill none
	stroke #424242
	stroke-width 2
	stroke-linecap round
	stroke-miterlimit 10

#contour .st2
	fill none
	stroke #424242
	stroke-width 1.3675
	stroke-linecap round
	stroke-miterlimit 10

#contour .st3
	fill none
	stroke #424242
	stroke-width 1.3675
	stroke-miterlimit 10

#contour .st4
	fill none 
	stroke #424242
	stroke-width 2
	stroke-miterlimit 10

