	30% { transform : translateZ(100px) scale(1); opacity: 1; }
	60% {opacity: 1;}
	to 	{ transform: translateX(0px) translateY(0px) translateZ(0px); opacity: 0; }

@keyframes cadreE
	from { transform: translateX(0px) translateY(0px) translateZ(180px); opacity: 1; }
	30% { transform : translateZ(120px) scale(1); opacity: 1; }
	60% {opacity: 1;}        
	to 	{ transform: translateX(0px) translateY(0px) translateZ(0px); opacity: 0; }
