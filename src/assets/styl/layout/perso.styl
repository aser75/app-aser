#cadre-visage,
#cadre-oeil,
#cadre-cache,
#cadre-bonnet,
#cadre-stylo,
#cadre-bouche
	position absolute
	left 0
	right 0
	top 0
	width 100%

#cadre-visage
	transform-style preserve-3d
	animation cadreVisage 3500ms cubic-bezier(0.645, 0.045, 0.355, 1)
	opacity 0

#cadre-bouche
	transform-style preserve-3d
	animation cadreA 3500ms cubic-bezier(0.645, 0.045, 0.355, 1)
	opacity 0

#cadre-oeil
	transform-style preserve-3d
	animation cadreB 3500ms cubic-bezier(0.645, 0.045, 0.355, 1)
	opacity 0

#cadre-cache
	transform-style preserve-3d
	animation cadreC 3500ms cubic-bezier(0.645, 0.045, 0.355, 1)
	opacity 0

#cadre-bonnet
	transform-style preserve-3d
	animation cadreD 3500ms cubic-bezier(0.645, 0.045, 0.355, 1)
	opacity 0

#cadre-stylo
	transform-style preserve-3d
	animation cadreE 3500ms cubic-bezier(0.645, 0.045, 0.355, 1)
	opacity 0

@keyframes cadreVisage
	from { opacity: 1; }
	30% { opacity: 1; }
	60% {opacity: 1;}
	to 	{ opacity: 0; }

@keyframes cadreA
	from { transform: translateX(0px) translateY(0px) translateZ(60px); opacity: 1; }
	30% { transform : translateZ(20px) scale(1); opacity: 1; }
	60% {opacity: 1;}
	to 	{ transform: translateX(0px) translateY(0px) translateZ(0px); opacity: 0; }

@keyframes cadreB
	from { transform: translateX(0px) translateY(0px) translateZ(90px); opacity: 1; }
	30% { transform : translateZ(40px) scale(1); opacity: 1; }
	60% {opacity: 1;}
	to 	{ transform: translateX(0px) translateY(0px) translateZ(0px); opacity: 0; }

@keyframes cadreC
	from { transform: translateX(0px) translateY(0px) translateZ(120px); opacity: 1; }
	30% { transform : translateZ(80px) scale(1); opacity: 1; }
	60% {opacity: 1;}        
	to 	{ transform: translateX(0px) translateY(0px) translateZ(0px); opacity: 0; }

@keyframes cadreD
